   2449          /*********************************************************************
   2450           * @fn      ZDO_MatchEndDeviceBind()
   2451           *
   2452           * @brief
   2453           *
   2454           *   Called to match end device binding requests
   2455           *
   2456           * @param  bindReq  - binding request information
   2457           * @param  SecurityUse - Security enable/disable
   2458           *
   2459           * @return  none
   2460           */
   2461          void ZDO_MatchEndDeviceBind( ZDEndDeviceBind_t *bindReq )
   2462          {
   2463            zAddrType_t dstAddr;
   2464            uint8 sendRsp = FALSE;
   2465            uint8 status;
   2466          
   2467            // Is this the first request?
   2468            if ( matchED == NULL )
   2469            {
   2470              // Create match info structure
   2471              matchED = (ZDMatchEndDeviceBind_t *)osal_mem_alloc( sizeof ( ZDMatchEndDeviceBind_t ) );
   2472              if ( matchED )
   2473              {
   2474                // Clear the structure
   2475                osal_memset( (uint8 *)matchED, 0, sizeof ( ZDMatchEndDeviceBind_t ) );
   2476          
   2477                // Copy the first request's information
   2478                if ( !ZDO_CopyMatchInfo( &(matchED->ed1), bindReq ) )
   2479                {
   2480                  status = ZDP_NO_ENTRY;
   2481                  sendRsp = TRUE;
   2482                }
   2483              }
   2484              else
   2485              {
   2486                status = ZDP_NO_ENTRY;
   2487                sendRsp = TRUE;
   2488              }
   2489          
   2490              if ( !sendRsp )
   2491              {
   2492                // Set into the correct state
   2493                matchED->state = ZDMATCH_WAIT_REQ;
   2494          
   2495                // Setup the timeout
   2496                APS_SetEndDeviceBindTimeout( AIB_MaxBindingTime, ZDO_EndDeviceBindMatchTimeoutCB );
   2497              }
   2498            }
   2499            else
   2500            {
   2501              if(matchED->state == ZDMATCH_WAIT_REQ)//when bind is running,refuse new end-device-bind-req
   2502              {
   2503                matchED->state = ZDMATCH_SENDING_BINDS;
   2504          
   2505                // Copy the 2nd request's information
   2506                if ( !ZDO_CopyMatchInfo( &(matchED->ed2), bindReq ) )
   2507                {
   2508                  status = ZDP_NO_ENTRY;
   2509                  sendRsp = TRUE;
   2510                }
   2511          
   2512                // Make a source match for ed1
   2513                matchED->ed1numMatched = ZDO_CompareClusterLists(
   2514                matchED->ed1.numOutClusters, matchED->ed1.outClusters,
   2515                matchED->ed2.numInClusters, matchED->ed2.inClusters, ZDOBuildBuf );
   2516                if ( matchED->ed1numMatched )
   2517                {
   2518                  // Save the match list
   2519                  matchED->ed1Matched = osal_mem_alloc( (short)(matchED->ed1numMatched * sizeof ( uint16 )) );
   2520                  if ( matchED->ed1Matched )
   2521                  {
   2522                    osal_memcpy( matchED->ed1Matched, ZDOBuildBuf, (matchED->ed1numMatched * sizeof ( uint16 )) );
   2523                  }
   2524                  else
   2525                  {
   2526                    // Allocation error, stop
   2527                    status = ZDP_NO_ENTRY;
   2528                    sendRsp = TRUE;
   2529                  }
   2530                }
   2531          
   2532                // Make a source match for ed2
   2533                matchED->ed2numMatched = ZDO_CompareClusterLists(
   2534                matchED->ed2.numOutClusters, matchED->ed2.outClusters,
   2535                matchED->ed1.numInClusters, matchED->ed1.inClusters, ZDOBuildBuf );
   2536                if ( matchED->ed2numMatched )
   2537                {
   2538                  // Save the match list
   2539                  matchED->ed2Matched = osal_mem_alloc( (short)(matchED->ed2numMatched * sizeof ( uint16 )) );
   2540                  if ( matchED->ed2Matched )
   2541                  {
   2542                    osal_memcpy( matchED->ed2Matched, ZDOBuildBuf, (matchED->ed2numMatched * sizeof ( uint16 )) );
   2543                  }
   2544                  else
   2545                  {
   2546                    // Allocation error, stop
   2547                    status = ZDP_NO_ENTRY;
   2548                    sendRsp = TRUE;
   2549                  }
   2550                }
   2551          
   2552                if ( (sendRsp == FALSE) && (matchED->ed1numMatched || matchED->ed2numMatched) )
   2553                {
   2554                  // Do the first unbind/bind state
   2555                  ZDMatchSendState( ZDMATCH_REASON_START, ZDP_SUCCESS, 0 );
   2556                }
   2557                else
   2558                {
   2559                  status = ZDP_NO_MATCH;
   2560                  sendRsp = TRUE;
   2561                }
   2562              }
   2563              else
   2564              {
   2565                status = ZDP_NO_ENTRY;
   2566                dstAddr.addrMode = Addr16Bit;
   2567                dstAddr.addr.shortAddr = bindReq->srcAddr;
   2568                ZDP_EndDeviceBindRsp( bindReq->TransSeq, &dstAddr, status, bindReq->SecurityUse );
   2569                return;
   2570              }
   2571            }
   2572          
   2573            if ( sendRsp )
   2574            {
   2575              // send response to this requester
   2576              dstAddr.addrMode = Addr16Bit;
   2577              dstAddr.addr.shortAddr = bindReq->srcAddr;
   2578              ZDP_EndDeviceBindRsp( bindReq->TransSeq, &dstAddr, status, bindReq->SecurityUse );
   2579          
   2580              if ( matchED->state == ZDMATCH_SENDING_BINDS )
   2581              {
   2582                // send response to first requester
   2583                dstAddr.addrMode = Addr16Bit;
   2584                dstAddr.addr.shortAddr = matchED->ed1.srcAddr;
   2585                ZDP_EndDeviceBindRsp( matchED->ed1.TransSeq, &dstAddr, status, matchED->ed1.SecurityUse );
   2586              }
   2587          
   2588              // Process ended - release memory used
   2589              ZDO_RemoveMatchMemory();
   2590            }
   2591          }
