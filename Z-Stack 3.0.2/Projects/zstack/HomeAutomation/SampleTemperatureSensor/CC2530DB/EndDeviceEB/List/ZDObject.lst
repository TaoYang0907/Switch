   2662              if ( destReq->inClusters )
   2663              {
   2664                // Copy the clusters
   2665                osal_memcpy( (uint8*)(destReq->inClusters), (uint8 *)(srcReq->inClusters),
   2666                                (srcReq->numInClusters * sizeof ( uint16 )) );
   2667              }
   2668              else
   2669              {
   2670                allOK = FALSE;
   2671              }
   2672            }
   2673          
   2674            // Copy output cluster IDs
   2675            if ( srcReq->numOutClusters )
   2676            {
   2677              destReq->outClusters = osal_mem_alloc( (short)(srcReq->numOutClusters * sizeof ( uint16 )) );
   2678              if ( destReq->outClusters )
   2679              {
   2680                // Copy the clusters
   2681                osal_memcpy( (uint8 *)(destReq->outClusters), (uint8 *)(srcReq->outClusters),
   2682                                (srcReq->numOutClusters * sizeof ( uint16 )) );
   2683              }
   2684              else
   2685              {
   2686                allOK = FALSE;
   2687              }
   2688            }
   2689          
   2690            if ( allOK == FALSE )
   2691            {
   2692              if ( destReq->inClusters != NULL )
   2693              {
   2694                osal_mem_free( destReq->inClusters );
   2695              }
   2696              if ( destReq->outClusters != NULL )
   2697              {
   2698                osal_mem_free( destReq->outClusters );
   2699              }
   2700            }
   2701          
   2702            return ( allOK );
