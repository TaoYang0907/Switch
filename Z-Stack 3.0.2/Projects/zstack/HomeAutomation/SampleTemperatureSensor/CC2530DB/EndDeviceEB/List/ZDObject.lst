   2606            {
   2607              if ( matchED->ed2Matched != NULL )
   2608              {
   2609                osal_mem_free( matchED->ed2Matched );
   2610              }
   2611              if ( matchED->ed1Matched != NULL )
   2612              {
   2613                osal_mem_free( matchED->ed1Matched );
   2614              }
   2615              if ( matchED->ed1.inClusters != NULL )
   2616              {
   2617                osal_mem_free( matchED->ed1.inClusters );
   2618              }
   2619              if ( matchED->ed1.outClusters != NULL )
   2620              {
   2621                osal_mem_free( matchED->ed1.outClusters );
   2622              }
   2623              if ( matchED->ed2.inClusters != NULL )
   2624              {
   2625                osal_mem_free( matchED->ed2.inClusters );
   2626              }
   2627              if ( matchED->ed2.outClusters != NULL )
   2628              {
   2629                osal_mem_free( matchED->ed2.outClusters );
   2630              }
