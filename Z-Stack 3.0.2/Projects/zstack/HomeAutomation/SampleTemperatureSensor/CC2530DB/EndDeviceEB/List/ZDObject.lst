   2653          
   2654            // Initialize the destination cluster pointers
   2655            destReq->inClusters = NULL;
   2656            destReq->outClusters = NULL;
   2657          
   2658            // Copy input cluster IDs
   2659            if ( srcReq->numInClusters )
   2660            {
