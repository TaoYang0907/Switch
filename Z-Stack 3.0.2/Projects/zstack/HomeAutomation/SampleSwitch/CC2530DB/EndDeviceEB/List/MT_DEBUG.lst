    142          static void MT_DebugSetThreshold(uint8 *pBuf)
    143          {
    144            uint8 retValue = ZSuccess;
    145            uint8 cmdId;
    146          
    147            /* parse header */
    148            cmdId = pBuf[MT_RPC_POS_CMD1];
    149            pBuf += MT_RPC_FRAME_HDR_SZ;
    150          
