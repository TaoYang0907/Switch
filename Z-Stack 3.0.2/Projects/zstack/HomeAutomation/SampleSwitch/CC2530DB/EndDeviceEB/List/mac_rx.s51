//  265 
//  266   /* clear any accidental threshold interrupt that happened as part of power up sequence */
//  267   MAC_RADIO_CLEAR_RX_THRESHOLD_INTERRUPT_FLAG();
        MOV       C,0xa8.7
        CLR       A
