        MOV       DPTR,#pFuncRxState
        MOV       A,#`??rxStartIsr::?relay` & 0xff
        MOVX      @DPTR,A
        INC       DPTR
        MOV       A,#(`??rxStartIsr::?relay` >> 8) & 0xff
        MOVX      @DPTR,A
//  244   macRxOutgoingAckFlag = 0;
